{
    "name": "order-execution-system",
    "version": "1.0.0",
    "description": "Order execution engine with DEX routing and WebSocket status updates",
    "main": "dist/server.js",
    "scripts": {
        "build": "tsc",
        "start": "ts-node src/server.ts",
        "dev": "nodemon --exec ts-node src/server.ts",
        "type-check": "tsc --noEmit",
        "migrate": "node migrations/run_migration.js",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage"
    },
    "keywords": [
        "order-execution",
        "dex-router",
        "websocket",
        "fastify",
        "bullmq"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@fastify/websocket": "^11.2.0",
        "@solana/spl-token": "^0.4.8",
        "@solana/web3.js": "^1.95.2",
        "bs58": "^6.0.0",
        "bullmq": "^5.4.0",
        "dotenv": "^16.4.5",
        "fastify": "^5.6.2",
        "ioredis": "^5.3.4",
        "pg": "^8.12.0",
        "uuid": "^9.0.1",
        "ws": "^8.18.3"
    },
    "devDependencies": {
        "@types/jest": "^29.5.14",
        "@types/node": "^20.11.30",
        "@types/pg": "^8.11.6",
        "@types/uuid": "^9.0.8",
        "@types/ws": "^8.18.1",
        "jest": "^29.7.0",
        "nodemon": "^3.1.11",
        "ts-jest": "^29.4.5",
        "ts-node": "^10.9.2",
        "typescript": "^5.4.5"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}
